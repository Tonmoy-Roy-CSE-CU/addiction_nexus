@prefix an: <https://zenodo.org/records/16881403#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix qb4o: <http://purl.org/qb4olap/cubes#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

an:MH_Dataset a qb:DataSet,
        owl:Class ;
    qb:structure an:MH_Cuboid .

an:dim_Country a qb:DimensionProperty,
        owl:Class ;
    rdfs:label "Country Dimension (covers ['Continent', 'Country'])" .

an:dim_Gender a qb:DimensionProperty,
        owl:Class ;
    rdfs:label "Gender Dimension (covers [])" .

an:dim_MentalHealth a qb:DimensionProperty,
        owl:Class ;
    rdfs:label "MentalHealth Dimension (covers ['Condition', 'Severity', 'ConsultationHistory', 'Stress', 'Medication'])" .

an:dim_Occupation a qb:DimensionProperty,
        owl:Class ;
    rdfs:label "Occupation Dimension (covers [])" .

an:hier_Lifestyle a qb4o:Hierarchy,
        owl:Class ;
    rdfs:label "Lifestyle Hierarchy" ;
    qb4o:hasLevel an:lev_Lifestyle_Alcohol,
        an:lev_Lifestyle_Diet,
        an:lev_Lifestyle_Smoking ;
    qb4o:inDimension an:dim_Lifestyle .

an:step_Country_Country_to_Continent a qb4o:HierarchyStep ;
    qb4o:childLevel an:lev_Country_Continent ;
    qb4o:inHierarchy an:hier_Country ;
    qb4o:parentLevel an:lev_Country_Country ;
    qb4o:pcCardinality qb4o:OneToMany ;
    qb4o:rollup an:inContinent .

an:step_MentalHealth_Condition_to_Severity a qb4o:HierarchyStep ;
    qb4o:childLevel an:lev_MentalHealth_Severity ;
    qb4o:inHierarchy an:hier_MentalHealth ;
    qb4o:parentLevel an:lev_MentalHealth_Condition ;
    qb4o:pcCardinality qb4o:OneToMany ;
    qb4o:rollup an:inSeverity .

an:MH_Cuboid a qb:DataStructureDefinition,
        owl:Class ;
    dcterms:conformsTo an:Ontology ;
    qb:component [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_MentalHealth_ConsultationHistory ],
        [ qb:measure an:meas_Sleep_Hours ;
            qb4o:aggregateFunction qb4o:avg,
                qb4o:count,
                qb4o:sum ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_Lifestyle_Alcohol ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_MentalHealth_Condition ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_Country_Country ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_MentalHealth_Medication ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_MentalHealth_Stress ],
        [ qb:measure an:meas_Age ;
            qb4o:aggregateFunction qb4o:avg,
                qb4o:count,
                qb4o:sum ],
        [ qb:measure an:meas_Social_Media_Usage ;
            qb4o:aggregateFunction qb4o:avg,
                qb4o:count,
                qb4o:sum ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_Lifestyle_Smoking ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_Country_Continent ],
        [ qb:measure an:meas_Physical_Activity_Hours ;
            qb4o:aggregateFunction qb4o:avg,
                qb4o:count,
                qb4o:sum ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_Lifestyle_Diet ],
        [ qb:measure an:meas_Work_Hours ;
            qb4o:aggregateFunction qb4o:avg,
                qb4o:count,
                qb4o:sum ],
        [ qb4o:cardinality qb4o:OneToMany ;
            qb4o:level an:lev_MentalHealth_Severity ] .

an:Ontology a owl:Ontology ;
    dcterms:created "2025-08-15"^^xsd:date ;
    dcterms:creator "University of Chittagong" ;
    dcterms:description "A multidimensional behavioral, neurophysiological and socioeconomic determinants of addiction cartography plays a vital role in addiction research. This study combines OLAP analytics with knowledge graph technology to provide structured insights into dependency patterns using a comprehensive dataset of 50,000 mental health and lifestyle records. While earlier research has explored semantic modeling and OLAP techniques, many studies have inadequately addressed FAIR compliance validation, limiting interoperability and accessibility." ;
    dcterms:identifier "ArticleID-1419" ;
    dcterms:license "CC BY 4.0" ;
    dcterms:relation "https://zenodo.org/records/16881403" ;
    dcterms:title "Addiction Nexus: A Multidimensional Cartography of Behavioral, Neurophysiological, and Socioeconomic Determinants of Dependency Using OLAP and Knowledge Graphs" ;
    rdfs:seeAlso "Ahmad et al. (2023)",
        "Chaudhuri & Dayal (1997)",
        "Chen et al. (2024)",
        "Etcheverry & Vaisman (2012)",
        "Johnson & Brown (2024)",
        "Rodriguez et al. (2023)",
        "Smith et al. (2023)",
        "Wilkinson et al. (2016)",
        "Williams et al. (2022)" .

